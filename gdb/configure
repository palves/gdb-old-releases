else
  echo "$ac_t""no" 1>&6
echo $ac_n "checking for -ldld""... $ac_c" 1>&6
ac_lib_var=`echo dld_main | tr '.-/+' '___p'`
if eval "test \"`echo '$''{'ac_cv_lib_$ac_lib_var'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  ac_save_LIBS="$LIBS"
